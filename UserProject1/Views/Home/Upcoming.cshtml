
@{
    ViewData["Title"] = "Upcoming";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

<style type="text/css">
    body {
        background-image: url(https://img.freepik.com/free-photo/white-plain-wall-surface-background_53876-74688.jpg?size=626&ext=jpg );
        background-size: cover;
        background-attachment: fixed;
    }
</style>

<main role="main">

    <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">
            <div class="carousel-item active">
                <svg>  <img src="https://aniportalimages.s3.amazonaws.com/media/details/kalank.jpg"class="bd-placeholder-img" width="100%" height="40%"><rect width="100%" height="100%" fill="#777" /></svg>
                <div class="container">
                    <div class="carousel-caption text-left">

                    </div>
                </div>
            </div>
            <div class="carousel-item">
                <svg>  <img src="https://st1.bollywoodlife.com/wp-content/uploads/2018/09/Kesri.jpg" class="bd-placeholder-img" width="100%" height="40%"><rect width="100%" height="100%" fill="#777" /></svg>
                <div class="container">
                    <div class="carousel-caption">


                    </div>
                </div>
            </div>
            <div class="carousel-item">
                <svg>  <img src="https://images.news18.com/ibnlive/uploads/2019/02/badla.jpg" class="bd-placeholder-img" width="100%" height="40%"><rect width="100%" height="100%" fill="#777" /></svg>
                <div class="container">
                    <div class="carousel-caption text-right">


                    </div>
                </div>
            </div>
        </div>
        <a class="carousel-control-prev" href="#myCarousel" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#myCarousel" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</main>
